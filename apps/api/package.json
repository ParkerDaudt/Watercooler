    "@watercooler/shared": "workspace:*",
    "argon2": "^0.41.1",
    "drizzle-orm": "^0.45.1",
    "fastify": "^5.1.0",
    "jose": "^5.9.6",
    "nanoid": "^5.0.9",
    "open-graph-scraper": "^6.11.0",
    "postgres": "^3.4.5",
    "socket.io": "^4.8.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "drizzle-kit": "^0.31.9",
    "tsx": "^4.19.2",
    "typescript": "^5.5.0"
